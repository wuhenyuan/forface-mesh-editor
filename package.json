{
  "name": "forface-mesh-editor",
  "version": "1.0.0",
  "description": "Vue 2 + Vite 项目",
  "main": "dist/forface-mesh-editor.umd.js",
  "module": "dist/forface-mesh-editor.es.js",
  "exports": {
    ".": {
      "import": "./dist/forface-mesh-editor.es.js",
      "require": "./dist/forface-mesh-editor.umd.js"
    }
  },
  "files": [
    "dist",
    "src/components",
    "src/utils"
  ],
  "dependencies": {
    "element-ui": "^0.2.6",
    "jszip": "^3.10.1",
    "three": "^0.160.0",
    "three-bvh-csg": "^0.0.17",
    "three-mesh-bvh": "^0.9.4",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-vue2": "^2.0.3",
    "vue-template-compiler": "^2.7.16",
    "vue-tsc": "^3.2.1"
  },
  "scripts": {
    "dev": "vite --config vite.dev.config.ts",
    "build": "vite build --config vite.config.ts",
    "build:lib": "vite build --mode lib --config vite.config.ts",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vue",
    "vite",
    "mesh-editor"
  ],
  "author": "",
  "license": "ISC",
  "type": "module"
}
